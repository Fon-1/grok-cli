{
  "name": "grok-cli",
  "version": "0.1.0",
  "description": "Ask Grok when you're stuck. Bundle your prompt and files, send via Chrome automation to grok.com.",
  "type": "module",
  "main": "dist/bin/grok.js",
  "bin": {
    "grok": "dist/bin/grok.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx src/bin/grok.ts",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "test": "node --experimental-vm-modules node_modules/.bin/vitest run"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": ["grok", "xai", "cli", "browser-automation", "ai"],
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "chrome-launcher": "^1.1.2",
    "chrome-remote-interface": "^0.33.2",
    "clipboardy": "^4.0.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "fast-glob": "^3.3.2",
    "json5": "^2.2.3",
    "marked": "^12.0.0",
    "marked-terminal": "^7.1.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/marked-terminal": "^6.0.0",
    "@types/node": "^20.14.0",
    "tsx": "^4.15.7",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
